import os

nome_base='IndividuoWine1'

dcol = "/home/marcos/Documents/Tese/dcol/DCoL-v1.1/Source/dcol"
enderecoin = " -i /home/marcos/Documents/Tese/Distancias/ResultadosDistanciasValidaTeste/Wine/DistanciasWine0.arff"
